section(data-i18n-aria-label="skills")#skills
	.skills
		h2(data-i18n='skills')
		+input-label( {
			label:'untrained',
			inputObj: {
				name:'untrained',
				type:'number',
				value: -2,
				trigger:{
					affects:allSkillNames
					}
				}
			})

		.grid.tiny-gap
			+skillHeader
			each skill in primarySkills
				+skillRow(skill)
